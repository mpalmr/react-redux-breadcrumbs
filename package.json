{
  "name": "react-redux-breadcrumbs",
  "version": "0.0.0",
  "description": "Breadcrumb component for React with Redux",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "scripts": {
    "start": "npm run clean && npm run babel -- -w",
    "build": "npm run clean -- dist && npm run babel",
    "clean": "rimraf lib",
    "babel": "babel src -d lib -s --ignore .eslintrc.js",
    "sass": "node-sass src/styles/index.scss styles.css --source-map",
    "test": "cross-env BABEL_ENV=test nyc mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpalmr/react-redux-breadcrumbs.git"
  },
  "keywords": [
    "react",
    "redux",
    "component",
    "breadcrumbs"
  ],
  "author": "Matthew Palmer <mspalmer91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mpalmr/react-redux-breadcrumbs/issues"
  },
  "homepage": "https://github.com/mpalmr/react-redux-breadcrumbs#readme",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "power-assert": "^1.4.4",
    "rimraf": "^2.6.2",
    "shell-executor": "^4.0.0"
  }
}
